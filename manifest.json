{
  "name": "Translate",
  "version": "1.0",
  "description": "Translate by Rayman",
  "manifest_version": 2,
  "permissions": [],
  "background" : {
    "scripts" : [
      "background.js"
    ],
    "persistent" : false
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": [
        "content.js"
      ]
    }
  ]
}